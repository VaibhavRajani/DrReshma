  <script>
    document.addEventListener("DOMContentLoaded", function () {
      const bulletLinks = document.querySelectorAll(".bullet-link");
      const whiteBoxes = document.querySelectorAll(".white-box");

      bulletLinks.forEach(function (link) {
        link.addEventListener("click", function (e) {
          e.preventDefault();
          const target = e.target.getAttribute("href");
          showContent(target);
          setActiveLink(link);
        });
      });

      function showContent(target) {
        const contentBoxes = document.querySelectorAll(".white-box");

        contentBoxes.forEach(function (box) {
          const content = getContent(target, box.id);
          box.innerHTML = content.html;
          box.style.backgroundColor = content.backgroundColor;
        });
      }

      function setActiveLink(link) {
        bulletLinks.forEach(function (link) {
          link.classList.remove("active");
        });
        link.classList.add("active");
      }

      function getContent(target, boxId) {
        switch (target) {
          case "#box-1":
            switch (boxId) {
              case "box-1":
                return {
                  html:
                    '<img src="Vaginal_Surgery.png" alt="Logo" class="services">' +
                    '<p class="topic pt-2 cardtext">Vaginal Surgery</p>' +
                    '<p class="carddescription">' +
                    'Surgical procedures performed on the vaginal region, such as vaginal reconstruction or repair.' +
                    '</p>'
                };

              case "box-2":
                return {
                  html: 
					'<img src="Operative_Laparoscopy.png" alt="Logo" class="services">' +
					'<p class="topic pt-2 cardtext">Operative Laproscopy</p>' +
					'<p class="carddescription">' + 
					'Surgical procedures performed on the vaginal region, such as vaginal reconstruction or repair.' + 
					'</p>'
                };
              case "box-3":
                return {
                  html: 
					'<img src="Tube_Litigation.png" alt="Logo" class="services">' +
					'<p class="topic pt-2 cardtext">Tube Litigation</p>' +
					'<p class="carddescription"> Permanent contraception method involving the closure or blocking of the fallopian tubes to prevent pregnancy.' +
					'</p>',
                };
              case "box-4":
                return {
                  html: 
					'<img src="AP_Repair.png" alt="Logo" class="services">' +
					'<p class="topic pt-2 cardtext">Hysteroscopy with AP repair</p>' +
					'<p class="carddescription"> Diagnostic and surgical procedure to examine the uterus and perform repairs, such as addressing uterine abnormalities or polyps.' + 
					'</p>',
                };
              default:
                return {
                  html: 
					'<img src="Hysterectomy.png" alt="Logo" class="services">' +
					'<p class="topic pt-2 cardtext">Hysterectomy</p>' +
					'<p class="carddescription"> Surgical removal of the uterus.' +
					'</p>'
                };
            }
          case "#box-2":
            switch (boxId) {
              case "box-1":
                return {
                  html: '<img src=".png" alt="Logo" class="services">' +
					'<p class="topic pt-2 cardtext"> High-Risk Obstetrics </p>' +
					'<p class="carddescription"> Surgical procedures performed on the vaginal region, such as vaginal  care for pregnancies that have increased risks due to maternal or fetal conditions reconstruction or repair.' +
					'</p>'
                };
              case "box-2":
                return {
                  html: '<img src="Operative_Laparoscopy.png" alt="Logo" class="services">' +
					'<p class="topic pt-2 cardtext">Ante Natal Care</p>' +
					'<p class="carddescription">Medical care and monitoring provided during pregnancy to ensure the health of both the mother and the developing fetus. ' +
					'</p>'
                };
              case "box-3":
                return {
                  html: '<img src="Tube_Litigation.png" alt="Logo" class="services">' +
					'<p class="topic pt-2 cardtext">Normal Delivery</p>' +
					'<p class="carddescription"> Vaginal delivery without the need for medical interventions or surgical procedures.' +
					'</p>'
                };
              case "box-4":
                return {
                  html: '<img src="AP_Repair.png" alt="Logo" class="services">' +
					'<p class="topic pt-2 cardtext">Complex Obstetrics</p>' +
					'<p class="carddescription"> Management of pregnancies with multiple complications or complex medical conditions.' +
					'</p>'
                };
              default:
                return {
                  html: '<img src=".png" alt="Logo" class="services">' +
					'<p class="topic pt-2 cardtext">Ectopic Pregnancy</p>' +
					'<p class="carddescription"> A pregnancy that occurs outside the uterus, typically in the fallopian tube, requiring medical intervention.' +
					'</p>'
                };
            }
          case "#box-3":
            switch (boxId) {
              case "box-1":
                return {
                  html: '<img src=".png" alt="Logo" class="services">' +
					'<p class="topic pt-2 cardtext">Infertility Management</p>' +
					'<p class="carddescription">Diagnosis and treatment of infertility, including assessment of fertility potential and appropriate interventions.' +
					'</p>'
                };
              case "box-2":
                return {
                  html: '<img src="Operative_Laparoscopy.png" alt="Logo" class="services">' +
					'<p class="topic pt-2 cardtext">Contraceptive Options</p>' +
					'<p class="carddescription">Counseling and provision of various birth control methods to prevent unwanted pregnancies.' +
					'</p>'
                };
              case "box-3":
                return {
                  html: '<img src=".png" alt="Logo" class="services">' +
					'<p class="topic pt-2 cardtext">Tubal Litigation</p>' +
					'<p class="carddescription">Noncancerous growths in the uterus that can cause symptoms such as heavy menstrual bleeding or pelvic pain.' +
					'</p>'
                };
              case "box-4":
                return {
                  html: '<img src="AP_Repair.png" alt="Logo" class="services">' +
					'<p class="topic pt-2 cardtext">Polycystic Ovarian Management</p>' +
					'<p class="carddescription">Treatment and management of polycystic ovary syndrome (PCOS), a hormonal disorder affecting the ovaries.' +
					'</p>'
                };
              default:
                return {
                  html: '<img src="Hysterectomy.png" alt="Logo" class="services">' +
					'<p class="topic pt-2 cardtext">Ovarian Cyst</p>' +
					'<p class="carddescription">Fluid-filled sacs that form on or within the ovaries, which may require monitoring or treatment.' +
					'</p>'
                };
            }
          case "#box-4":
            switch (boxId) {
              case "box-1":
                return {
                  html: '<img src="Vaginal_Surgery.png" alt="Logo" class="services">' +
					'<p class="topic pt-2 cardtext">Menstrual Irregularities</p>' +
					'<p class="carddescription">Assessment and management of abnormalities in the menstrual cycle, such as heavy or irregular periods.' +
					'</p>'
                };
              case "box-2":
                return {
                  html: '<img src="Operative_Laparoscopy.png" alt="Logo" class="services">' +
					'<p class="topic pt-2 cardtext">Chronic Pelvic Pain</p>' +
					'<p class="carddescription">Diagnosis and management of persistent pain in the pelvic region.' +
					'</p>'
                };
              case "box-3":
                return {
                  html: '<img src="Pelvic.png" alt="Logo" class="services">' +
					'<p class="topic pt-2 cardtext">Management of Pelvic Pain</p>' +
					'<p class="carddescription">Assessment and treatment of various conditions causing pelvic pain, such as endometriosis or pelvic inflammatory disease.' +
					'</p>'
                };
              case "box-4":
                return {
                  html: '<img src="Breast_Complaint.png" alt="Logo" class="services">' +
					'<p class="topic pt-2 cardtext">Breast Complaint Management</p>' +
					'<p class="carddescription">Evaluation and treatment of breast-related concerns, including breast lumps, pain, or abnormalities. ' +
					'</p>'
                };
              default:
                return {
                  html: '<img src="Hysterectomy.png" alt="Logo" class="services">' +
					'<p class="topic pt-2 cardtext">Menopause</p>' +
					'<p class="carddescription">Support and management of the physiological changes and symptoms experienced during the transition into menopause. ' +
					'</p>'
                };
            }
          default:
            return {
              html: '<p>Default HTML content</p>'
            };
        }
      }
    });

  </script>

